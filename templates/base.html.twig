            .then(data => {
                souspaysSelect.innerHTML = '';
                data.forEach(function (souspays) {
                    var option = document.createElement('option');
                    option.value = souspays.id;
                    option.text = souspays.name;
                    souspaysSelect.appendChild(option);

                });
            });
        // Vérifiez si le DataTable existe déjà et détruisez-le
        if ($.fn.DataTable.isDataTable('#basic-datatable')) {
            $('#basic-datatable').DataTable().destroy();
        }

        // Initialisez à nouveau le DataTable
        $('#basic-datatable').DataTable({
            'paging': true,
            'lengthChange': false,
            'searching': true,
            'ordering': true,
            'info': true,
            'autoWidth': false,
            /*"scrollY":        "500px",*/
            "scrollCollapse": true,
            "language": {
                "url": 'French.json'
            },
        });
    });
    $('#produit_IDPAYS').change( function () {

        var idPays = this.value;
        var souspaysSelect = document.getElementById('produit_SEQSOUSPAYS');

        var url = '{{ path('get_souspays') }}';
        fetch(url + '?IDPAYS=' + idPays)
            .then(response => response.json())
            .then(data => {
                souspaysSelect.innerHTML = '';
                data.forEach(function (souspays) {
                    var option = document.createElement('option');
                    option.value = souspays.id;
                    option.text = souspays.name;
                    souspaysSelect.appendChild(option);

                });
            });
    });
</script>
